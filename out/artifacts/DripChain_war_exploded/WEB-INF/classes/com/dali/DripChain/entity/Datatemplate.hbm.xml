<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dali.DripChain.entity.Datatemplate" table="tb_datatemplate" schema="db_dripchain">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="sTemplateName">
            <column name="sTemplateName" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>