<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dali.DripChain.entity.DeviceslavesList" table="deviceslaves_list" schema="db_dripchain">
        <composite-id mapped="true" class="com.dali.DripChain.entity.DeviceslavesListPK">
            <key-property name="deviceId">
                <column name="Device_id" sql-type="int(11)"/>
            </key-property>
            <key-property name="listIndex">
                <column name="list_index" sql-type="int(11)"/>
            </key-property>
        </composite-id>
        <property name="deviceSlavesId">
            <column name="deviceSlaves_id" sql-type="int(11)"/>
        </property>
    </class>
</hibernate-mapping>