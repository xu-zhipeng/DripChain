<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dali.DripChain.entity.TbUserinfo" table="tb_userinfo" schema="db_dripchain">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="iUserId">
            <column name="iUserId" sql-type="int(11)"/>
        </property>
        <property name="sRealname">
            <column name="sRealname" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sSex">
            <column name="sSex" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sTelephone">
            <column name="sTelephone" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sEmail">
            <column name="sEmail" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sAddress">
            <column name="sAddress" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sCompanyName">
            <column name="sCompanyName" sql-type="varchar(255)" not-null="true"/>
        </property>
        <property name="sRemark">
            <column name="sRemark" sql-type="varchar(255)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>